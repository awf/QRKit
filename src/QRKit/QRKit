#ifndef QRKIT_SPARSE_QR_EXTRA_MODULE_H
#define QRKIT_SPARSE_QR_EXTRA_MODULE_H

#include "Eigen/Sparse"

// #include "../../Eigen/src/Core/util/DisableStupidWarnings.h"

#include <vector>
#include <map>
#include <cstdlib>
#include <cstring>
#include <algorithm>
#include <fstream>
#include <sstream>

/**
  * \defgroup SparseQR_Module SparseQR module
  *
  * This module extends the Eigen default SparseQR with special QR 
  * solvers for block diagonal, row banded matrices and block angular.
  *
  * \code
  * #include <QRKit/QRKit>
  * \endcode
  */

#include "QRKit/SparseQRUtils.h"
#include "QRKit/SparseQROrdering.h"
#include "QRKit/BlockYTY.h"
#include "QRKit/SparseBlockCOO.h"
#include "QRKit/SparseBlockDiagonal.h"
#include "QRKit/BlockMatrix1x2.h"
#include "QRKit/SparseBlockYTY.h"
#include "QRKit/BlockDiagonalSparseQR.h"
#include "QRKit/BandedBlockedSparseQR.h"
#include "QRKit/BlockedThinSparseQR.h"
#include "QRKit/BlockedThinDenseQR.h"
#include "QRKit/BlockAngularSparseQR.h"

// #include "../../Eigen/src/Core/util/ReenableStupidWarnings.h"

#endif // EIGEN_SPARSE_QR_EXTRA_MODULE_H